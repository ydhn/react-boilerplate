    "moment": "^2.22.0",
    "node-sass": "^4.9.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router-dom": "4.3.1",
    "reactstrap": "6.3.1"
  },

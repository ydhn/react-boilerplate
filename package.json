    "moment": "^2.21.0",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "react": "^16.3.0-alpha.3",
    "react-dom": "^16.3.0-alpha.3",
    "react-router-dom": "^4.2.2",
    "react-stickynode": "^1.4.1",
    "reactstrap": "^5.0.0-beta.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "@babel/core": "^7.0.0-beta.40",

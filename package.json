{
  "name": "react-boilerplate",
  "version": "0.0.2",
  "author": "Yundo Han",
  "license": "UNLICENSED",
  "homepage": "http://yun.do",
  "description": "React 16.3 Boilerplate.",
  "main": "server.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack --progress --mode=development && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback --port 5000",
    "dist": "webpack --progress --mode=production && ncp src/index.html dist/index.html",
    "surge": "npm run dist && cd ./dist && cp index.html 200.html && surge -p ./ -d http://url",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.3",
    "@material-ui/icons": "^2.0.1",
    "bootstrap": "4.1.3",

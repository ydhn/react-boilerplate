{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "author": "Yundo Han",
  "license": "UNLICENSED",
  "homepage": "http://yun.do",
  "description": "React 16.3 Boilerplate.",
  "main": "server.js",
  "scripts": {
    "start": "npm run build",
    "build": "rimraf dist && webpack --progress -d && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback --port 5000",
    "dist": "rimraf dist && webpack --progress -p && ncp src/index.html dist/index.html",
    "surge": "npm run dist && cd ./dist && cp index.html 200.html && surge -p ./ -d http://url",
    "test": "jest --coverage"
  },
  "dependencies": {
    "bootstrap": "4.0.0",
    "history": "^4.7.2",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
